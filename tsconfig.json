{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"target": "ES2022",
		"strict":true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true
	},
	"include": [
		"AllTheBots.ts",
		"Commands.ts",
		"test.ts",
		"Pokebot/*.ts",
		"custom_modules/*.ts"
	]
}